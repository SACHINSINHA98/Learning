package PrefixSum;

public class MinSwaps {
public static void main(String[] args) {
	int []arr= {10,4,8,7,3,8,-1,8,8};
	System.out.println(swaps(arr));
}

private static int swaps(int[] arr) {
	int swap=0;
	int count=0;
	for(int i=0;i<arr.length;i++) {
		if(arr[i]==8) {
			count++;
		}
	}
	return swap;
}
}
